#include <iostream>

using namespace std;

int main()
{
    setlocale(LC_ALL, "");
    float a, b;
    cout << "Podaj A: ";
    cin >> a;
    cout << "Podaj B: ";
    cin >> b;

    if (b == 0)
		{
			cout << "Nie wolno dzieliÄ‡ przez zero!";
		}
		else
		{
			cout << "Iloraz: " << (a / b) << endl;
		}
    return 0;
}

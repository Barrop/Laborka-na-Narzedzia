#include <iostream>

using namespace std;

int main()
{
    setlocale(LC_ALL, "");
    float a, b;
    cout << "Podaj A: " << endl; // zmienna A
    cin >> a;
    cout << "Podaj B: " << endl; // zmienna B
    cin >> b;

    if (b == 0)
		{
			cout << "Nie wolno dzieliÄ‡ przez zero!" << endl; // Warunek
		}
    else
		{
			cout << "Iloraz: " << (a / b) << endl; // Iloraz
		}
    return 0;
}
